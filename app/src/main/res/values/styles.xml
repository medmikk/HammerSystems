        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/grey_light</item>
        <item name="android:fontFamily">@font/sf_light</item>

    </style>

    <style name="text_grey_dark_bold_16sp" parent="Widget.AppCompat.TextView">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/grey_dark</item>
